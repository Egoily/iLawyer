<UserControl x:Class="ee.iLawyer.Modules.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:usercontrols="clr-namespace:ee.iLawyer.UserControls" 
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:vm="clr-namespace:ee.iLawyer.ViewModels"
             mc:Ignorable="d" 
             
             >
    <Grid x:Name="grid">

        <StackPanel>
            <usercontrols:PropertyListPicker x:Name="propertyListPicker"  Width="600" 
                DataSource="{Binding Path=DataContext.PersonProperties,ElementName=grid,  Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
            />
            <Button Click="Button_Click">OK</Button>





        </StackPanel>

    </Grid>
</UserControl>
